auth-user-pass /config/openvpn-credentials.txt
remote norway.vpn.blackvpn.com 443 tcp
nobind
client
dev tun
fast-io
persist-key
persist-tun
cipher AES-256-CBC
txqueuelen 486
sndbuf 1655368
rcvbuf 1655368
auth SHA512
pull
comp-lzo
tls-client
key-method 2
ns-cert-type server
ca /etc/openvpn/blackvpn/ssl/ca.crt
tls-auth /etc/openvpn/blackvpn/ssl/ta.key 1
verb 3
mute 10
script-security 2

